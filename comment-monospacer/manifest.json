{
  "content_scripts": [{
    "css": ["style.css"],
    "matches": [
      "*://chromiumcodereview.appspot.com/*",
      "*://codereview.chromium.org/*"
    ]
  }],
  "description": "Makes Chromium codereview comments monospace",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "manifest_version": 2,
  "name": "Comment Monospacer",
  "version": "1.0"
}
