{
  "content_scripts": [{
    "js": ["script.js"],
    "matches": [
      "*://codereview.chromium.org/*",
      "*://github.com/*"
    ]
  }],
  "description": "Protects you from accidentally closing in-progress code review comments",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "manifest_version": 2,
  "name": "Comment Protector",
  "version": "1.2"
}
